{
	"manifest_version": 2,
	"name": "WhiteBuster",
	"short_name": "WhiteBuster",
	"version": "1.3.1",
	"key": "",

	"description": "Busting whites since 2017",
	"browser_action": {
		"default_icon": "whitebuster128.png",
		"default_popup": "popup.html"
	},

	"content_scripts": [
		{
			"js": ["whitebuster.js"],
			"matches": ["*://*/*"],
			"all_frames": true
		}
	],

	"background": {
		"scripts": ["background.js"]
	},

	"permissions": [
		"storage",
		"http://*/*", 
		"https://*/*"
	],

	"icons": { 
		"16": "whitebuster16.png",
		"48": "whitebuster48.png",
		"128": "whitebuster128.png" 
	}
}
